    },
    "require-dev": {
        "phpunit/phpunit": "^10",
        "monolog/monolog": "2.*",
        "phpstan/phpstan": "^1.10",
        "captainhook/captainhook": "^5.16",
        "squizlabs/php_codesniffer": "^3.7"
    }
}
